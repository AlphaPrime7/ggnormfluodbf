test_package:
	test_package <- function (package = "./", ...) {
		devtools::document(package)
		devtools::test(package, ...)
	}
	test_package()

bar_data:
	bar_data <- function () {
		df1 <- data.frame(sex = rep(c("Male", "Female", "Other", "Transfaux"),
		each = 100), smoker = factor(rep(c("Yes", "No"), times = 200)))
		df1
	}
	bar_data()
